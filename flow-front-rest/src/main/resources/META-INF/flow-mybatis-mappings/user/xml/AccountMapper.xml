<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dragon.flow.mapper.user.IAccountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dragon.flow.model.user.Account">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="username" property="username" />
        <result column="pwd" property="pwd" />
        <result column="tel" property="tel" />
        <result column="email" property="email" />
        <result column="name" property="name" />
        <result column="create_time" property="createTime" />
        <result column="creator" property="creator" />
        <result column="update_time" property="updateTime" />
        <result column="updator" property="updator" />
        <result column="del_flag" property="delFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, username, pwd, tel, email, name, create_time, creator, update_time, updator, del_flag
    </sql>

</mapper>
